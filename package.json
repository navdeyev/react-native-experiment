{
  "name": "react-native-experiment",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.1.0",
    "@types/react-native": "^0.52.21",
    "@types/react-navigation": "^1.5.11",
    "@types/react-redux": "^6.0.0",
    "@types/react-test-renderer": "^16.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest-expo": "26.0.0",
    "react-dom": "16.3.2",
    "react-native-scripts-ts": "^1.14.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.3.0-alpha.1",
    "ts-jest": "^22.4.2",
    "tslib": "^1.9.0",
    "typescript": "^2.8.1"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.ts",
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "expo": "27.0.2",
    "react": "16.3.1",
    "react-native": "^0.55.4",
    "react-navigation": "1.5.12",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.4"
  }
}
